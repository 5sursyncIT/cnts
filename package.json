{
  "name": "cnts-monorepo",
  "private": true,
  "workspaces": [
    "web",
    "portal",
    "mobile",
    "packages/*"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "lint": "npm -w web run lint && npm -w portal run lint && npm -ws --if-present run lint",
    "test": "npm -ws --if-present run test",
    "test:coverage": "npm -ws --if-present run test:coverage",
    "dev:web": "npm -w web run dev",
    "dev:portal": "npm -w portal run dev",
    "mfa:backoffice": "node ./scripts/backoffice-mfa.mjs"
  },
  "overrides": {
    "react": "19.2.3",
    "react-dom": "19.2.4",
    "jest": "29.7.0"
  },
  "dependencies": {
    "expo-linking": "~8.0.11"
  }
}
